<h1>Resenas#index</h1>
<p>Find me in app/views/resenas/index.html.erb</p>

<ul>
      
      <div style="background-color: #71a9c0; width:500px; height:auto;border: 3px solid;box-shadow: 5px 5px;margin:auto;">
        <% @usuario.resenas.each do |resena| %>
            <span><%= resena.id %></span>
            <span>&#8226;</span>Calificación: <%= resena.calificacion %> <br/>
            <span>&#8226;</span>Descripción: <%= resena.contenido %> <br/>
            <span><%= resena.autor.nombre %></span>
            <% if resena.autor_id == current_usuario.id %>
              <div style="display: inline-block;">
                <%= link_to "Eliminar reseña", resena_path(@usuario.id, resena.id), method: :delete %>
                <%= link_to "Editar reseña", edit_resena_path(@usuario.id, resena.id) %>
              </div>
            <% end %>
            <br/>
        <% end %>
      </div>
  </ul>
   <ul>
    <%= link_to "Nueva reseña", new_resena_path, params: {autor_id: current_usuario.id, usuario_id: @usuario.id} %>
  </ul>
